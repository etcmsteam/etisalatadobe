<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:tags="[]"
        cq:template="/conf/etisalat/settings/wcm/templates/xf-web-variation"
        cq:xfMasterVariation="{Boolean}true"
        cq:xfVariantType="web"
        jcr:primaryType="cq:PageContent"
        jcr:title="Header"
        sling:resourceType="etisalat/components/xfpage">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="etisalat/components/container"
            layout="simple">
            <navigation
                jcr:primaryType="nt:unstructured"
                sling:resourceType="etisalat/components/navigation"
                collectAllPages="false"
                navigationRoot="/content/etisalat/us/en"
                skipNavigationRoot="false"
                structureDepth="1"/>
            <languagenavigation
                jcr:primaryType="nt:unstructured"
                sling:resourceType="etisalat/components/languagenavigation"
                navigationRoot="/content/etisalat/us"
                structureDepth="2"/>
            <search
                jcr:primaryType="nt:unstructured"
                sling:resourceType="etisalat/components/search"
                searchRoot="/content/etisalat/us"/>
				 <container
                jcr:primaryType="nt:unstructured"
                sling:resourceType="etisalat/components/container"
                layout="responsiveGrid">
                <image
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="etisalat/components/image"
                    altValueFromDAM="false"
                    displayPopupTitle="true"
                    fileReference="/content/dam/etisalat/asset.jpg"
                    isDecorative="false"
                    titleValueFromDAM="false">
                    <cq:responsive jcr:primaryType="nt:unstructured">
                        <default
                            jcr:primaryType="nt:unstructured"
                            offset="0"
                            width="3"/>
                    </cq:responsive>
                </image>
                <list
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="etisalat/components/list"
                    childDepth="2"
                    linkItems="false"
                    listFrom="children"
                    parentPage="/content/etisalat/us/en/pages"
                    showDescription="false"
                    showModificationDate="false"
                    sortOrder="asc"
                    tagsMatch="any">
                    <cq:responsive jcr:primaryType="nt:unstructured">
                        <default
                            jcr:primaryType="nt:unstructured"
                            offset="3"
                            width="6"/>
                    </cq:responsive>
                </list>
            </container>
        </root>
    </jcr:content>
</jcr:root>
