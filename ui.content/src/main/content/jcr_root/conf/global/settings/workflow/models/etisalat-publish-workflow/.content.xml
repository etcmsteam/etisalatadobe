<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:designPath="/libs/settings/wcm/designs/default"        
        cq:template="/libs/cq/workflow/templates/model"
        jcr:primaryType="cq:PageContent"
        jcr:title="Etisalat Publish Workflow"
        sling:resourceType="cq/workflow/components/pages/model"
        lastSynced="{Date}2022-07-12T16:52:31.448Z">
        <flow
            jcr:primaryType="nt:unstructured"
            sling:resourceType="foundation/components/parsys">
            <dynamic_participant                
                jcr:description="Get approval from reviewer's group."                
                jcr:primaryType="nt:unstructured"
                jcr:title="Reviewer's Group Approval"
                sling:resourceType="cq/workflow/components/model/dynamic_participant">
                <metaData
                    jcr:primaryType="nt:unstructured"
                    DO_NOTIFY="true"
                    DYNAMIC_PARTICIPANT="/apps/workflow/scripts/etisalat/etisalat-dynamic-participant.ecma"
                    timeoutHandler="com.adobe.granite.workflow.console.timeout.autoadvance.AutoAdvancer"
                    timeoutMillis="1"/>
            </dynamic_participant>
            <participant               
                jcr:primaryType="nt:unstructured"
                jcr:title="Ops Group Approval"
                sling:resourceType="cq/workflow/components/model/participant">
                <metaData
                    jcr:primaryType="nt:unstructured"
                    DO_NOTIFY="true"
                    PARTICIPANT="Etisalat CMS IT team"
                    timeoutHandler="com.adobe.granite.workflow.console.timeout.autoadvance.AutoAdvancer"
                    timeoutMillis="1"/>
            </participant>
        </flow>
    </jcr:content>
</jcr:root>
