<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Google Analytics Details"
          sling:resourceType="granite/ui/components/foundation/layouts/collapsible">
    <items jcr:primaryType="nt:unstructured">
        <gaeventname
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
            fieldDescription="An event name for Google Analytics data."
            fieldLabel="Google Analytics Event"
            name="./gaeventname"/>
        <gaeventcategory
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
            fieldDescription="An event category for Google Analytics data."
            fieldLabel="Google Analytics Event Category"
            name="./gaeventcategory"/>
    </items>
</jcr:root>
