<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Footer"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <promotion
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Promotion Logo"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <container
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <backgroundcolor
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/include"
                                                path="/apps/etisalat/components/eandenterprise/commons/editor/dialog/backgroundcolor/backgroundcolor"/>
                                            <file
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                allowUpload="{Boolean}false"
                                                autoStart="{Boolean}false"
                                                class="cq-droptarget"
                                                fieldLabel="Promotion Logo Image"
                                                fileNameParameter="./promo/fileName"
                                                fileReferenceParameter="./promo/fileReference"
                                                mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                multiple="{Boolean}false"
                                                name="./promo/file"
                                                title="Upload Image Asset"
                                                uploadUrl="${suffix.path}"
                                                useHTML5="{Boolean}true"/>
                                            <alttext
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Alt Text"
                                                fieldLabel="Alt Text"
                                                name="./altText"
                                                required="{Boolean}false"/>
                                        </items>
                                    </container>
                                </items>
                            </columns>
                        </items>
                    </promotion>
                    <followus
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Follow Us"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <container
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <followussectiontitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Follow us section title"
                                                fieldLabel="Follow us section title"
                                                name="./followUsTitle"
                                                required="{Boolean}false"/>
                                            <followussection
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                fieldLabel="Follow us section"
                                                required="{Boolean}false">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                    name="./followus">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <followusurl
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                            fieldDescription="Follow Us URL"
                                                            fieldLabel="Follow Us URL"
                                                            name="./linkUrl"
                                                            required="{Boolean}true"
                                                            rootPath="/content"/>
                                                        <followuslinktarget
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                            checked="{Boolean}false"
                                                            fieldDescription="If checked, the URL will open in new tab."
                                                            fieldLabel="Open in new tab"
                                                            name="./linkTarget"
                                                            text="Open in new tab"
                                                            uncheckedValue="false"
                                                            value="true"/>
                                                        <title
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="Enter title"
                                                            fieldLabel="Title"
                                                            name="./title"
                                                            required="{Boolean}true"/>
                                                        <file
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                            allowUpload="{Boolean}false"
                                                            autoStart="{Boolean}false"
                                                            class="cq-droptarget"
                                                            fieldLabel="Follow us Logo Image"
                                                            fileNameParameter="./fileName"
                                                            fileReferenceParameter="./fileReference"
                                                            mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                            multiple="{Boolean}false"
                                                            name="./file"
                                                            title="Upload Image Asset"
                                                            uploadUrl="${suffix.path}"
                                                            useHTML5="{Boolean}true"/>
                                                    </items>
                                                </field>
                                            </followussection>
                                        </items>
                                    </container>
                                </items>
                            </columns>
                        </items>
                    </followus>
                    <footerlinks
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Footer Links"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <container
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <linksection
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                fieldLabel="Add footer links"
                                                required="{Boolean}false">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                    name="./footerLinks">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <linktext
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="Link Text"
                                                            fieldLabel="Link Text"
                                                            name="./linkText"
                                                            required="{Boolean}true"/>
                                                        <linkurl
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                            fieldDescription="Link URL"
                                                            fieldLabel="Link URL"
                                                            name="./linkUrl"
                                                            required="{Boolean}true"
                                                            rootPath="/content"/>
                                                        <linktarget
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                            checked="{Boolean}false"
                                                            fieldDescription="If checked, the URL will open in new tab."
                                                            fieldLabel="Open in new tab"
                                                            name="./linkTarget"
                                                            text="Open in new tab"
                                                            uncheckedValue="false"
                                                            value="true"/>
                                                    </items>
                                                </field>
                                            </linksection>
                                        </items>
                                    </container>
                                </items>
                            </columns>
                        </items>
                    </footerlinks>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
