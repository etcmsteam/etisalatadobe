<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
     jcr:primaryType="nt:unstructured"
     jcr:title="1440px Image"
     sling:resourceType="granite/ui/components/coral/foundation/container"
     margin="{Boolean}true">
     <items jcr:primaryType="nt:unstructured">
         <columns
             jcr:primaryType="nt:unstructured"
             sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
             margin="{Boolean}true">
             <items jcr:primaryType="nt:unstructured">
                 <column
                     jcr:primaryType="nt:unstructured"
                     sling:resourceType="granite/ui/components/coral/foundation/container">
                     <items jcr:primaryType="nt:unstructured">
                         <retina1440px_1ximage
                             jcr:primaryType="nt:unstructured"
                             sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                             allowUpload="{Boolean}false"
                             autoStart="{Boolean}false"
                             class="cq-droptarget"
                             fieldLabel="1440px 1x Image"
                             fileNameParameter="./retinaimage1440px/1x/fileName"
                             fileReferenceParameter="./retinaimage1440px/1x/fileReference"
                             mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                             multiple="{Boolean}false"
                             name="./retinaimage1440px/1x/file"
                             title="Upload Image Asset"
                             uploadUrl="${suffix.path}"
                             useHTML5="{Boolean}true"/>
                         <retina1440px_2ximage
                             jcr:primaryType="nt:unstructured"
                             sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                             allowUpload="{Boolean}false"
                             autoStart="{Boolean}false"
                             class="cq-droptarget"
                             fieldLabel="1440px 2x Image"
                             fileNameParameter="./retinaimage1440px/2x/fileName"
                             fileReferenceParameter="./retinaimage1440px/2x/fileReference"
                             mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                             multiple="{Boolean}false"
                             name="./retinaimage1440px/2x/file"
                             title="Upload Image Asset"
                             uploadUrl="${suffix.path}"
                             useHTML5="{Boolean}true"/>
                         <alttext
                             jcr:primaryType="nt:unstructured"
                             sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                             fieldDescription="Image Alt text"
                             fieldLabel="Image Alt text"
                             name="./retinaimage1440px/altText"/>
                     </items>
                 </column>
             </items>
         </columns>
     </items>
    <granite:data
       jcr:primaryType="nt:unstructured"
       dep-value="4-viewport"/>
</jcr:root>
