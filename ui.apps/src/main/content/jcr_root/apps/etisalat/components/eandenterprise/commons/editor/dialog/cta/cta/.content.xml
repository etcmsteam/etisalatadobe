<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    sling:resourceType="granite/ui/components/coral/foundation/container"
    margin="{Boolean}true">
    <items jcr:primaryType="nt:unstructured">
        <columns
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
            margin="{Boolean}true">
            <items jcr:primaryType="nt:unstructured">
                <column
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/container">
                    <items jcr:primaryType="nt:unstructured">
                        <ctaStyle
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/select"
                            fieldDescription="Select CTA Style"
                            fieldLabel="CTA Style"
                            name="./ctaStyle">
                            <items jcr:primaryType="nt:unstructured">
                                <default
                                    jcr:primaryType="nt:unstructured"
                                    text="Default"
                                    value="default"/>
                                <learnMoreButton
                                    jcr:primaryType="nt:unstructured"
                                    text="Learn More Button"
                                    value="learn-more-btn"/>
                                <primaryBtnMobileFullWidth
                                    jcr:primaryType="nt:unstructured"
                                    text="Primary Button Mobile Full With"
                                    value="primary-btn mobile-full-width"/>
                                <primaryButton
                                    jcr:primaryType="nt:unstructured"
                                    text="Primary Button"
                                    value="primary-btn"/>
                                <primaryButtonOutline
                                    jcr:primaryType="nt:unstructured"
                                    text="Primary Button Outline"
                                    value="primary-btn outline"/>
                                <primaryButtonOutlineDarkBg
                                    jcr:primaryType="nt:unstructured"
                                    text="Primary Button Outline Dark Bg"
                                    value="primary-btn outline dark-bg"/>
                                <primaryButtonDarkBg
                                    jcr:primaryType="nt:unstructured"
                                    text="Primary Button Dark Bg"
                                    value="primary-btn dark-bg"/>
                                <learnmorefaq
                                    jcr:primaryType="nt:unstructured"
                                    text="Learn more Faq"
                                    value="btn-text btn-green learn-more"/>
                            </items>
                        </ctaStyle>
                        <ctalabel
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldDescription="Add CTA Label"
                            fieldLabel="CTA Label"
                            maxlength="{Long}80"
                            name="./ctalabel"/>
                        <linkBehavior
                            granite:class="cq-dialog-dropdown-showhide"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/select"
                            fieldDescription="Select Link Behavior"
                            fieldLabel="Link Behavior"
                            name="./linkBehavior">
                            <items jcr:primaryType="nt:unstructured">
                                <opennewwindow
                                    jcr:primaryType="nt:unstructured"
                                    text="Open In New Window"
                                    value="opennewwindow"/>
                                <opensamewindow
                                    jcr:primaryType="nt:unstructured"
                                    text="Open in Same Window"
                                    value="opensamewindow"/>
                            </items>
                        </linkBehavior>
                        <ctalink
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                            fieldDescription="Target URL for when the link is clicked."
                            fieldLabel="CTA link"
                            name="./ctalink"
                            rootPath="/content"/>
                    </items>
                </column>
            </items>
        </columns>
    </items>
</jcr:root>
