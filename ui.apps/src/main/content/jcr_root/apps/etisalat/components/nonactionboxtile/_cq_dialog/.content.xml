<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Non Action Box Tile"
    sling:resourceType="cq/gui/components/authoring/dialog"
    helpPath="https://www.adobe.com/go/aem_cmp_title_v2">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <properties
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Non Action Box Details"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <newFeatures
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="Enter Value &quot;New Features&quot; text here."
                                                fieldDescription="Enter Value &quot;New Features&quot; text for the Left Right Variation."
                                                fieldLabel="New Features Text"
                                                name="./newFeatures"/>
                                            <newFeatureType
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Style for &quot;New Feature text&quot;"
                                                name="./newFeatureType">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <def
                                                        jcr:primaryType="nt:unstructured"
                                                        text="(default)"
                                                        value=""/>
                                                    <h1
                                                        jcr:primaryType="nt:unstructured"
                                                        text="H1"
                                                        value="h1"/>
                                                    <h2
                                                        jcr:primaryType="nt:unstructured"
                                                        text="H2"
                                                        value="h2"/>
                                                    <h3
                                                        jcr:primaryType="nt:unstructured"
                                                        text="H3"
                                                        value="h3"/>
                                                    <h4
                                                        jcr:primaryType="nt:unstructured"
                                                        text="H4"
                                                        value="h4"/>
                                                    <h5
                                                        jcr:primaryType="nt:unstructured"
                                                        text="H5"
                                                        value="h5"/>
                                                    <h6
                                                        jcr:primaryType="nt:unstructured"
                                                        text="H6"
                                                        value="h6"/>
                                                </items>
                                            </newFeatureType>
                                            <pretitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="A pretitle that will be displayed on the Non Action Box Tile"
                                                fieldLabel="Non Action Box Pretitle"
                                                name="./pretitle"/>
                                            <title
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="A title to display on the Non Action Box Tile"
                                                fieldLabel="Non Action Box Title"
                                                name="./title"/>
                                            <titleType
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Style for &quot;Non Action Box Title&quot;"
                                                name="./titleType">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <def
                                                        jcr:primaryType="nt:unstructured"
                                                        text="(default)"
                                                        value=""/>
                                                    <h1
                                                        jcr:primaryType="nt:unstructured"
                                                        text="H1"
                                                        value="h1"/>
                                                    <h2
                                                        jcr:primaryType="nt:unstructured"
                                                        text="H2"
                                                        value="h2"/>
                                                    <h3
                                                        jcr:primaryType="nt:unstructured"
                                                        text="H3"
                                                        value="h3"/>
                                                    <h4
                                                        jcr:primaryType="nt:unstructured"
                                                        text="H4"
                                                        value="h4"/>
                                                    <h5
                                                        jcr:primaryType="nt:unstructured"
                                                        text="H5"
                                                        value="h5"/>
                                                    <h6
                                                        jcr:primaryType="nt:unstructured"
                                                        text="H6"
                                                        value="h6"/>
                                                </items>
                                            </titleType>
                                            <descriptionParagraphStyle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Description Paragraph Style"
                                                name="./descParagraphStyle">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <def
                                                        jcr:primaryType="nt:unstructured"
                                                        text="(default)"
                                                        value=""/>
                                                    <p1
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Paragraph 1"
                                                        value="p1"/>
                                                    <p2
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Paragraph 2"
                                                        value="p2"/>
                                                    <p3
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Paragraph 3"
                                                        value="p3"/>
                                                    <p4
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Paragraph 4"
                                                        value="p4"/>                                                   
                                                </items>
                                            </descriptionParagraphStyle>
                                            <description
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceSuperType="etisalat/components/text/cq:dialog/content/items/tabs/items/properties/items/columns/items/column/items/text"
                                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                fieldDescription="Text that will appear for the Non Action Box Tile Description."
                                                fieldLabel="Description"
                                                name="./description"
                                                useFixedInlineToolbar="{Boolean}true"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </properties>
                    <video
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Video"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <youtube
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                fieldDescription="Enter the video embed URL."
                                                fieldLabel="Youtube Video Embed URL"
                                                name="./youTubeUrl"/>
                                            <play-inplace
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <granite:data jcr:primaryType="nt:unstructured"/>
                                                <items jcr:primaryType="nt:unstructured">
                                                    <playicon
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                        allowUpload="{Boolean}false"
                                                        autoStart="{Boolean}false"
                                                        class="cq-droptarget"
                                                        fieldDescription="Drag &amp; drop the play icon image."
                                                        fieldLabel="Play Icon"
                                                        fileNameParameter="./playIconName"
                                                        fileReferenceParameter="./playIconReference"
                                                        mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                        multiple="{Boolean}false"
                                                        name="./playIcon"
                                                        title="Upload Image Asset"
                                                        uploadUrl="${suffix.path}"
                                                        useHTML5="{Boolean}true"/>
                                                </items>
                                            </play-inplace>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </video>
                    <userdetails
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Quotes Details"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <userdetails
                                                granite:class="userdetails"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <username
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="User Name to be entered"
                                                        fieldLabel="User Name"
                                                        name="./userName"/>
                                                    <userdesignation
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="User Designation to be entered"
                                                        fieldLabel="User Designation"
                                                        name="./userDesignation"/>
                                                </items>
                                            </userdetails>
                                            <descQuotesParagraphStyle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Quotes Description Paragraph Style"
                                                name="./descQuotesStyle">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <def
                                                        jcr:primaryType="nt:unstructured"
                                                        text="(default)"
                                                        value=""/>
                                                    <p1
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Paragraph 1"
                                                        value="p1"/>
                                                    <p2
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Paragraph 2"
                                                        value="p2"/>
                                                    <p3
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Paragraph 3"
                                                        value="p3"/>
                                                    <p4
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Paragraph 4"
                                                        value="p4"/>                                                   
                                                </items>
                                            </descQuotesParagraphStyle>
                                            <description
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceSuperType="etisalat/components/text/cq:dialog/content/items/tabs/items/properties/items/columns/items/column/items/text"
                                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                fieldDescription="Text that will appear for the main product Description."
                                                fieldLabel="Description"
                                                name="./quotesdescription"
                                                useFixedInlineToolbar="{Boolean}true"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </userdetails>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
