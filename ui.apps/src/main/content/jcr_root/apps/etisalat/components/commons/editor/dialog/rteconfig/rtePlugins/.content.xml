<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured">
    <format
        jcr:primaryType="nt:unstructured"
        features="*"/>
    <justify
        jcr:primaryType="nt:unstructured"
        features="*"/>
    <lists
        jcr:primaryType="nt:unstructured"
        features="*"/>
    <paraformat
        jcr:primaryType="nt:unstructured"
        features="*">
        <formats jcr:primaryType="nt:unstructured">
            <default_p
                jcr:primaryType="nt:unstructured"
                description="Paragraph"
                tag="p"/>
            <default_h1
                jcr:primaryType="nt:unstructured"
                description="Heading 1"
                tag="h1"/>
            <default_h2
                jcr:primaryType="nt:unstructured"
                description="Heading 2"
                tag="h2"/>
            <default_h3
                jcr:primaryType="nt:unstructured"
                description="Heading 3"
                tag="h3"/>
            <default_h4
                jcr:primaryType="nt:unstructured"
                description="Heading 4"
                tag="h4"/>
            <default_h5
                jcr:primaryType="nt:unstructured"
                description="Heading 5"
                tag="h5"/>
            <default_h6
                jcr:primaryType="nt:unstructured"
                description="Heading 6"
                tag="h6"/>
            <default_blockquote
                jcr:primaryType="nt:unstructured"
                description="Quote"
                tag="blockquote"/>
            <default_pre
                jcr:primaryType="nt:unstructured"
                description="Preformatted"
                tag="pre"/>
        </formats>
    </paraformat>
    <links
        jcr:primaryType="nt:unstructured"
        features="*"/>
    <image
        jcr:primaryType="nt:unstructured"
        features="*"/>
    <findreplace
        jcr:primaryType="nt:unstructured"
        features="*"/>
    <subsuperscript
        jcr:primaryType="nt:unstructured"
        features="*"/>
    <spellcheck
        jcr:primaryType="nt:unstructured"
        features="*"/>
    <misctools
        jcr:primaryType="nt:unstructured"
        features="*">
        <specialCharsConfig jcr:primaryType="nt:unstructured">
            <chars jcr:primaryType="nt:unstructured">
                <copyright
                    jcr:primaryType="nt:unstructured"
                    entity="&amp;#169;"/>
                <trademark
                    jcr:primaryType="nt:unstructured"
                    entity="&amp;#8482;"/>
                <registered
                    jcr:primaryType="nt:unstructured"
                    entity="&amp;#174;"/>
                <emDash
                    jcr:primaryType="nt:unstructured"
                    entity="&amp;#8212;"/>
                <pound
                    jcr:primaryType="nt:unstructured"
                    entity="&amp;#163;"/>
                <nbsp
                    jcr:primaryType="nt:unstructured"
                    entity="&amp;#160;"/>
            </chars>
        </specialCharsConfig>
    </misctools>
    <styles
        jcr:primaryType="nt:unstructured"
        features="*">
        <styles jcr:primaryType="nt:unstructured">
            <buttondisabled
                jcr:primaryType="nt:unstructured"
                cssName="cmp-button--disabled"
                text="Button Disabled"/>
            <buttongrey
                jcr:primaryType="nt:unstructured"
                cssName="cmp-button--grey"
                text="Button Grey"/>
            <buttonprimary
                jcr:primaryType="nt:unstructured"
                cssName="cmp-button--primary"
                text="Button Primary"/>
            <buttonsecondary
                jcr:primaryType="nt:unstructured"
                cssName="cmp-button--secondary"
                text="Button Secondary"/>
            <fullwidthbutton
                jcr:primaryType="nt:unstructured"
                cssName="cmp-button--width_100"
                text="Secondary Button Full Width"/>
            <fontbig
                jcr:primaryType="nt:unstructured"
                cssName="font--big"
                text="Font Big"/>
            <fontmedium
                jcr:primaryType="nt:unstructured"
                cssName="font--med"
                text="Font Medium"/>
            <fontsmall
                jcr:primaryType="nt:unstructured"
                cssName="font--small"
                text="Font Small"/>
            <fontxsmall
                jcr:primaryType="nt:unstructured"
                cssName="font--xsmall"
                text="Font X-Small"/>
            <fontcolorblack
                jcr:primaryType="nt:unstructured"
                cssName="text--black"
                text="Font Color Black"/>
            <fontcolorgray
                jcr:primaryType="nt:unstructured"
                cssName="text--gray"
                text="Font Color Gray"/>
            <fontcolorgreen
                jcr:primaryType="nt:unstructured"
                cssName="text--green"
                text="Font Color Green"/>
            <fontcolororange
                jcr:primaryType="nt:unstructured"
                cssName="text--orange"
                text="Font Color Orange"/>
            <fontcolorwhite
                jcr:primaryType="nt:unstructured"
                cssName="text--white "
                text="Font Color White"/>
            <linkarrow
                jcr:primaryType="nt:unstructured"
                cssName="cmp-link__arrow"
                text="Link Arrow"/>
            <whitelinkarrow
                jcr:primaryType="nt:unstructured"
                cssName="cmp-link__arrow cmp-link__arrow--white"
                text="White Link Arrow "/>
            <linkarrowlefticon
                jcr:primaryType="nt:unstructured"
                cssName="cmp-link__arrow_left"
                text="Link Arrow Left Icon"/>
            <linktext
                jcr:primaryType="nt:unstructured"
                cssName="cmp-link__text"
                text="Link Text"/>
            <listarrow
                jcr:primaryType="nt:unstructured"
                cssName="cmp-icon__arrow__list"
                text="List Style Arrow"/>
            <number
                jcr:primaryType="nt:unstructured"
                cssName="font--number"
                text="Number"/>
            <oldprice
                jcr:primaryType="nt:unstructured"
                cssName="old-price"
                text="Old Price"/>
            <oldpricevalue
                jcr:primaryType="nt:unstructured"
                cssName="old-price-value"
                text="Old Price Value"/>
            <spacebottom
                jcr:primaryType="nt:unstructured"
                cssName="space-bottom"
                text="Space Botttom"/>
            <spaceleft
                jcr:primaryType="nt:unstructured"
                cssName="space-left"
                text="Space Left"/>
            <spaceright
                jcr:primaryType="nt:unstructured"
                cssName="space-right"
                text="Space Right"/>
            <spacetop
                jcr:primaryType="nt:unstructured"
                cssName="space-top"
                text="Space Top"/>            
        </styles>
    </styles>
    <table
        jcr:primaryType="nt:unstructured"
        features="*"/>
</jcr:root>
