<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Store Locator"
          sling:resourceType="cq/gui/components/authoring/dialog"
          helpPath="https://www.adobe.com/go/aem_cmp_title_v2">
    <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/tabs"
                    maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <details
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Details"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                    margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <heading
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Enter the display heading."
                                                    fieldLabel="Heading"
                                                    name="./heading"/>
                                            <backgroundImageLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Enter the Background Image Alt text."
                                                    fieldLabel="Background Image Alt text"
                                                    name="./backgroundImageLabel"/>
                                            <searchplaceholdertext
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Enter the placeholder text appear on the search box."
                                                    fieldLabel="Search Box Placeholder Text"
                                                    name="./searchplaceholdertext"/>
                                            <locationhreftextlabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Href Text that will appear next to location icon."
                                                    fieldLabel="Location CTA Label"
                                                    name="./locationhreftextlabel"/>
                                            <locationsearchbuttontext
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Text that will appear on search button."
                                                    fieldLabel="Search Button Label"
                                                    name="./locationsearchbuttontext"/>
                                            <paginationLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Text that will appear on pagination."
                                                    fieldLabel="Pagination Label"
                                                    name="./paginationLabel"/>
                                            <ctaButtonText
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Text that will appear for the CTA label"
                                                    fieldLabel="CTA Text"
                                                    name="./ctaButtonText"/>
                                            <ctaButtonlink
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                    fieldDescription="CTA Link"
                                                    fieldLabel="CTA Link"
                                                    name="./ctaButtonlink"
                                                    rootPath="/content"/>
                                            <filtercategory
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="cq/gui/components/coral/common/form/tagfield"
                                                    autocreateTag="{Boolean}true"
                                                    fieldDescription="Choose Section Tag."
                                                    fieldLabel="Store Locator Section Tag"
                                                    name="./productFilterTag"
                                                    rootPath="/content/cq:tags/etisalat"/>
                                            <mobileCoverageLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Text that will appear on Coverage Section."
                                                    fieldLabel="Mobile Coverage Label"
                                                    name="./mobileCoverageLabel"/>
                                            <locationMapViewLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Text that will appear on MapView Tab."
                                                    fieldLabel="Map View Tab Label"
                                                    name="./locationMapViewLabel"/>
                                            <locationListViewLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Text that will appear on List view tab."
                                                    fieldLabel="List View Tab Label"
                                                    name="./locationListViewLabel"/>
                                            <storeLocationBodyDescription
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceSuperType="etisalat/components/commons/editor/dialog/rteconfig/text"
                                                    sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                    fieldDescription="Text that will appear for the main Description on mobile coverage section."
                                                    fieldLabel="Body Description"
                                                    name="./storeLocationBodyDescription"
                                                    useFixedInlineToolbar="{Boolean}true"/>
                                            <closeButtonLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Enter the text that will appear on close button."
                                                    fieldLabel="Close Label Button"
                                                    name="./closeButtonLabel"/>
                                            <showOnMapLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Enter the text that will appear on Show on Map link."
                                                    fieldLabel="Show On Map Label"
                                                    name="./showOnMapLabel"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </details>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
