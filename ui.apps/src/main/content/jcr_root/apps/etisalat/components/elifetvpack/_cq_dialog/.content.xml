<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="ELife TV Packs"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        granite:class="cmp-teaser__editor"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <packdetails
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Modal Details"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <channeltitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="A title to display as the headline for the TV Pack Channel."
                                                fieldLabel="Channel Package Title"
                                                name="./channelTitle"/>
                                            <channelTitleStyle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldDescription="Select Channel Package Title Style"
                                                fieldLabel="Channel Package Title Style"
                                                name="./channelTitleStyle">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <def
                                                        jcr:primaryType="nt:unstructured"
                                                        text="(default)"
                                                        value=""/>
                                                    <h1
                                                        jcr:primaryType="nt:unstructured"
                                                        text="H1"
                                                        value="h1"/>
                                                    <h2
                                                        jcr:primaryType="nt:unstructured"
                                                        text="H2"
                                                        value="h2"/>
                                                    <h3
                                                        jcr:primaryType="nt:unstructured"
                                                        text="H3"
                                                        value="h3"/>
                                                    <h4
                                                        jcr:primaryType="nt:unstructured"
                                                        text="H4"
                                                        value="h4"/>
                                                    <h5
                                                        jcr:primaryType="nt:unstructured"
                                                        text="H5"
                                                        value="h5"/>
                                                    <h6
                                                        jcr:primaryType="nt:unstructured"
                                                        text="H6"
                                                        value="h6"/>
                                                </items>
                                            </channelTitleStyle>
                                            <packdata
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <guideitem
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                        composite="{Boolean}true"
                                                        fieldLabel="Add TV Pack Item(s)">
                                                        <field
                                                            granite:class="coral-Well"
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/container"
                                                            name="./tvPackItems">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <file
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                                    allowUpload="{Boolean}false"
                                                                    autoStart="{Boolean}false"
                                                                    class="cq-droptarget"
                                                                    fieldLabel="TV Pack Image Asset"
                                                                    fileNameParameter="./fileName"
                                                                    fileReferenceParameter="./fileReference"
                                                                    mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                                    multiple="{Boolean}false"
                                                                    name="./file"
                                                                    title="Upload TV Pack Image Asset"
                                                                    uploadUrl="${suffix.path}"
                                                                    useHTML5="{Boolean}true">
                                                                    <channelLabel
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldDescription="Add View Channel CTA Link Label"
                                                                        fieldLabel="Channel CTA Link Label"
                                                                        name="./ctatext"/>
                                                                    <channelCTALink
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                                                        <items jcr:primaryType="nt:unstructured">
                                                                            <channellink
                                                                                jcr:primaryType="nt:unstructured"
                                                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                                fieldDescription="Target URL for when the link is clicked."
                                                                                fieldLabel="Channel CTA Link"
                                                                                name="./channelCTALink"
                                                                                rootPath="/content"/>
                                                                        </items>
                                                                    </channelCTALink>
                                                                </file>
                                                                <packagetitle
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldDescription="A title to display as the headline for the package."
                                                                    fieldLabel="TV Package Title"
                                                                    name="./packageTitle"/>
                                                                <packageTitleStyle
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                                    fieldDescription="Select Package Title Style"
                                                                    fieldLabel="TV Package Title Style"
                                                                    name="./packageTitleStyle">
                                                                    <items jcr:primaryType="nt:unstructured">
                                                                        <def
                                                                            jcr:primaryType="nt:unstructured"
                                                                            text="(default)"
                                                                            value=""/>
                                                                        <h1
                                                                            jcr:primaryType="nt:unstructured"
                                                                            text="H1"
                                                                            value="h1"/>
                                                                        <h2
                                                                            jcr:primaryType="nt:unstructured"
                                                                            text="H2"
                                                                            value="h2"/>
                                                                        <h3
                                                                            jcr:primaryType="nt:unstructured"
                                                                            text="H3"
                                                                            value="h3"/>
                                                                        <h4
                                                                            jcr:primaryType="nt:unstructured"
                                                                            text="H4"
                                                                            value="h4"/>
                                                                        <h5
                                                                            jcr:primaryType="nt:unstructured"
                                                                            text="H5"
                                                                            value="h5"/>
                                                                        <h6
                                                                            jcr:primaryType="nt:unstructured"
                                                                            text="H6"
                                                                            value="h6"/>
                                                                    </items>
                                                                </packageTitleStyle>
                                                                <channelLabel
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldDescription="Add View Channel CTA Link Label"
                                                                    fieldLabel="Channel CTA Link Label"
                                                                    name="./ctatext"/>
                                                                <channelCTALink
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                                                    <items jcr:primaryType="nt:unstructured">
                                                                        <channellink
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                            fieldDescription="Target URL for when the link is clicked."
                                                                            fieldLabel="Channel CTA Link"
                                                                            name="./channelCTALink"
                                                                            rootPath="/content"/>
                                                                    </items>
                                                                </channelCTALink>
                                                            </items>
                                                        </field>
                                                    </guideitem>
                                                </items>
                                            </packdata>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </packdetails>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
