<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Page"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[cq.common.wcm,core.wcm.components.page.v2.editor,cq.wcm.msm.properties,granite.contexthub.configuration,cq.siteadmin.admin.properties]"
    helpPath="https://www.adobe.com/go/aem_cmp_page_v2"
    mode="edit"
    trackingFeature="core-components:page:v2">
    <content
        granite:class="cq-dialog-content-page"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                granite:class="cq-siteadmin-admin-properties-tabs"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                size="L">
                <items jcr:primaryType="nt:unstructured">
                    <advanced
                        cq:showOnCreate="{Boolean}true"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Advanced"
                        sling:orderBefore="socialmedia"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <section1
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Settings"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <language
                                                granite:class="language"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                emptyText="Select"
                                                fieldLabel="Language"
                                                name="./jcr:language"
                                                renderReadOnly="{Boolean}true"
                                                translateOptions="{Boolean}true">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    cq-msm-lockable="jcr:language"/>
                                                <datasource
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="cq/gui/components/common/datasources/languages"
                                                    addNone="{Boolean}true"/>
                                            </language>
                                            <isLanguageRoot
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                fieldDescription="Must be checked if page is the root of a language copy."
                                                name="./cq:isLanguageRoot"
                                                renderReadOnly="{Boolean}true"
                                                text="Language Root"
                                                value="true">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    cq-msm-lockable="cq:isLanguageRoot"/>
                                            </isLanguageRoot>
                                            <redirect
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldLabel="Redirect"
                                                name="./cq:redirectTarget"
                                                nodeTypes="cq:Page"
                                                rootPath="/content">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    cq-msm-lockable="cq:redirectPath"/>
                                            </redirect>
                                            <designs
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="wcm/designer/gui/components/designfield"
                                                fieldLabel="Design"
                                                name="./cq:designPath"
                                                renderReadOnly="{Boolean}true">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    cq-msm-lockable="cq:designPath"/>
                                            </designs>
                                            <alias
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Alias"
                                                name="./sling:alias"
                                                renderReadOnly="{Boolean}true">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    cq-msm-lockable="sling:alias"/>
                                            </alias>
                                        </items>
                                    </section1>
                                    <configuration
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Configuration"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <cqConf
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/cloudconfig/components/admin/configpathbrowser"
                                                emptyText="Select Configuration Path"
                                                fieldLabel="Cloud Configuration">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    cq-msm-lockable="cq:conf"/>
                                            </cqConf>
                                        </items>
                                    </configuration>
                                    <spa
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <spa
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="SPA configuration"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <alias
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Remote APP URL"
                                                        name="./remoteURL"/>
                                                </items>
                                            </spa>
                                        </items>
                                        <granite:rendercondition
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/renderconditions/featuretoggle"
                                            toggleName="ft-cq-4303977"/>
                                    </spa>
                                    <templates
                                        cq:showOnCreate="{Boolean}false"
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Templates Settings"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <allowedtemplates
                                                granite:class="foundation-layout-util-maximized-alt"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                fieldDescription="Click 'Add Field' to add a template or a list of templates to be allowed as child pages. Each value in the list must be an absolute path to a template or use '/.*' to allow all templates below this path."
                                                fieldLabel="Allowed Templates"
                                                renderReadOnly="{Boolean}true">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    name="./cq:allowedTemplates"
                                                    renderReadOnly="{Boolean}true"/>
                                            </allowedtemplates>
                                        </items>
                                    </templates>
                                    <cssrequirement
                                        cq:showOnCreate="{Boolean}true"
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Custom CSS Requirements"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <allowedcss
                                                granite:class="foundation-layout-util-maximized-alt"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                fieldDescription="Click 'Add Field' to add a CSS file or a list of CSS files . Each value in the list must be an absolute path to a CSS file or use '/.*' to allow all CSS files below this path."
                                                fieldLabel="CSS to be added"
                                                renderReadOnly="{Boolean}true">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                    name="./customAllowedCss"
                                                    renderReadOnly="{Boolean}true"
                                                    rootPath="/content/dam/etisalat"/>
                                            </allowedcss>
                                        </items>
                                    </cssrequirement>
                                    <jsrequirement
                                        cq:showOnCreate="{Boolean}false"
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Custom JS Requirements"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <allowedjs
                                                granite:class="foundation-layout-util-maximized-alt"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                fieldDescription="Click 'Add Field' to add a JS file or a list of JS files . Each value in the list must be an absolute path to a JS file or use '/.*' to allow all JS files below this path."
                                                fieldLabel="JS to be added"
                                                renderReadOnly="{Boolean}true">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                    name="./customAllowedJs"
                                                    renderReadOnly="{Boolean}true"
                                                    rootPath="/content/dam/etisalat"/>
                                            </allowedjs>
                                        </items>
                                    </jsrequirement>
                                    <authenticationrequirement
                                        cq:showOnCreate="{Boolean}false"
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Authentication Requirement"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <cugconfigwarning
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/siteadmin/admin/properties/cugconfigwarning"
                                                warning="The Authentication Requirement cannot be edited because the page is using a deprecated configuration for the Closed User Group.&lt;br>Please refer to the &lt;a href=&quot;https://www.adobe.com/go/aem6_5_docs_cug_migration_en&quot; class=&quot;coral-Link&quot;>documentation&lt;/a>."/>
                                            <enable
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/siteadmin/admin/properties/authrequirement"
                                                fieldDescription="You might need to configure a Closed User Group in the Permission tab."
                                                text="Enable"/>
                                            <loginpath
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/siteadmin/admin/properties/loginpath"
                                                fieldLabel="Login Page"
                                                name="./cq:loginPath"
                                                rootPath="/content">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    cq-msm-lockable="cq:loginPath"/>
                                            </loginpath>
                                        </items>
                                    </authenticationrequirement>
                                    <export
                                        cq:showOnCreate="{Boolean}false"
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Export"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <exConfig
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldLabel="Export Configuration"
                                                name="./cq:exportTemplate"
                                                nodeTypes="nt:unstructured"
                                                optionLoader="cq.exportTemplate"
                                                renderReadOnly="{Boolean}true"
                                                rootPath="/etc/contentsync/templates">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    cq-msm-lockable="cq:exportTemplate"/>
                                            </exConfig>
                                        </items>
                                    </export>
                                    <seo
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="SEO -test"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <robotsTags
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldDescription="Select the the robots tags to control search engine crawler behavior. Keep in mind that some of the options conflict with each other. In that case the more permissive options take precedence."
                                                fieldLabel="Robots Tags"
                                                multiple="{Boolean}true"
                                                name="./cq:robotsTags">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <index
                                                        jcr:primaryType="nt:unstructured"
                                                        text="index"
                                                        value="index"/>
                                                    <noindex
                                                        jcr:primaryType="nt:unstructured"
                                                        text="noindex"
                                                        value="noindex"/>
                                                    <follow
                                                        jcr:primaryType="nt:unstructured"
                                                        text="follow"
                                                        value="follow"/>
                                                    <nofollow
                                                        jcr:primaryType="nt:unstructured"
                                                        text="nofollow"
                                                        value="nofollow"/>
                                                    <noarchive
                                                        jcr:primaryType="nt:unstructured"
                                                        text="noarchive"
                                                        value="noarchive"/>
                                                    <nosnippet
                                                        jcr:primaryType="nt:unstructured"
                                                        text="nosnippet"
                                                        value="nosnippet"/>
                                                    <noimageindex
                                                        jcr:primaryType="nt:unstructured"
                                                        text="noimageindex"
                                                        value="noimageindex"/>
                                                    <notranslate
                                                        jcr:primaryType="nt:unstructured"
                                                        text="notranslate"
                                                        value="notranslate"/>
                                                </items>
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    cq-msm-lockable="./cq:robotsTag"/>
                                            </robotsTags>
                                            <sitemapRoot
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                fieldDescription="If checked, a xml sitemap will be created for this page and its descendants."
                                                name="./sling:sitemapRoot"
                                                renderReadOnly="{Boolean}true"
                                                text="Generate Sitemap"
                                                value="true">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    cq-msm-lockable="./sling:sitemapRoot"/>
                                            </sitemapRoot>
                                        </items>
                                    </seo>
                                </items>
                            </column>
                        </items>
                    </advanced>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
