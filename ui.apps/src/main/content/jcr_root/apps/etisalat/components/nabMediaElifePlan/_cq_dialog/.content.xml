<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="NAB Media Elife Plan"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items
                    jcr:primaryType="nt:unstructured"
                    sling:hideChildren="[text,actions]">
                    <properties
                        jcr:primaryType="nt:unstructured"
                        jcr:title="NAB Content"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <nbasmalltitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Text that will appear for the NAB Media Elife Plan in small Font."
                                                fieldLabel="Pre Title"
                                                name="./nabSubTitle"/>
                                            <nbactatitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Text that will appear for the media Title."
                                                fieldLabel="Title"
                                                name="./nabTitle"/>
                                            <titleType
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Title Style"
                                                name="./titleType">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <def
                                                        jcr:primaryType="nt:unstructured"
                                                        text="(default)"
                                                        value=""/>
                                                    <h1
                                                        jcr:primaryType="nt:unstructured"
                                                        text="H1"
                                                        value="h1"/>
                                                    <h2
                                                        jcr:primaryType="nt:unstructured"
                                                        text="H2"
                                                        value="h2"/>
                                                    <h3
                                                        jcr:primaryType="nt:unstructured"
                                                        text="H3"
                                                        value="h3"/>
                                                    <h4
                                                        jcr:primaryType="nt:unstructured"
                                                        text="H4"
                                                        value="h4"/>
                                                    <h5
                                                        jcr:primaryType="nt:unstructured"
                                                        text="H5"
                                                        value="h5"/>
                                                    <h6
                                                        jcr:primaryType="nt:unstructured"
                                                        text="H6"
                                                        value="h6"/>
                                                </items>
                                            </titleType>
                                            <pricetag
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Price Tag"
                                                sling:resourceType="granite/ui/components/foundation/layouts/collapsible">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <productprice
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="Text that will appear for product price."
                                                        fieldLabel="Product Price"
                                                        maxlength="{Long}40"
                                                        name="./productPrice"/>
                                                    <productcurrency
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="Text that will appear for product charges/ currency."
                                                        fieldLabel="Product Currency/ Charges"
                                                        maxlength="{Long}40"
                                                        name="./productCurrency"/>
                                                    <pricefontcolor
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                        fieldDescription="Select Title Font Color"
                                                        fieldLabel="Price Font Color"
                                                        name="./priceFontColor">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <black
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Black"
                                                                value=""/>
                                                            <green
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Green"
                                                                value="green"/>
                                                        </items>
                                                    </pricefontcolor>
                                                    <oldprice
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="Text that will appear for old product rate."
                                                        fieldLabel="Old Price"
                                                        maxlength="{Long}40"
                                                        name="./oldPrice"/>
                                                    <vattext
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="Text that will appear for VAT info."
                                                        fieldLabel="VAT Text"
                                                        maxlength="{Long}40"
                                                        name="./vatText"/>
                                                </items>
                                            </pricetag>
                                            <description
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceSuperType="etisalat/components/commons/editor/dialog/rteconfig/text"
                                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                fieldDescription="Add Text for NAB Media Elife Plan Description"
                                                fieldLabel="Description"
                                                name="./description"
                                                useFixedInlineToolbar="{Boolean}true"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </properties>
                    <form
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Form"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <formDescription
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceSuperType="etisalat/components/commons/editor/dialog/rteconfig/text"
                                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                fieldDescription="Type the Form Description needed."
                                                fieldLabel="Form Heading &amp; Description"
                                                name="./formDescription"
                                                useFixedInlineToolbar="{Boolean}true"/>
                                            <formInputLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the label for the form input field."
                                                fieldLabel="Input Field Label"
                                                name="./formInputLabel"/>
                                            <inputID
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the ID for form input field."
                                                fieldLabel="Input Field ID"
                                                name="./inputID"/>
                                            <buttonLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the Button Label."
                                                fieldLabel="Button Label"
                                                name="./buttonLabel"/>
                                            <buttonType
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldDescription="Select the button type."
                                                fieldLabel="Button Type"
                                                name="./buttonType">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <submit
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Submit"
                                                        value="submit"/>
                                                    <button
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Button"
                                                        value="button"/>
                                                </items>
                                            </buttonType>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </form>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
